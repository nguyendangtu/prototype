    <jackson-databind-nullable.verison>0.2.6</jackson-databind-nullable.verison>
    <springdoc-openapi.version>2.1.0</springdoc-openapi.version>
    <maven.compiler.source>17</maven.compiler.source>
    <maven.compiler.target>17</maven.compiler.target>
    <wiremock.version>3.0.1</wiremock.version>
    <!-- Plugins Begin -->
    <maven-compiler-plugin.version>3.10.1</maven-compiler-plugin.version>
    <maven-failsafe-plugin.version>3.1.0</maven-failsafe-plugin.version>
    <maven-surefire-plugin.version>3.1.0</maven-surefire-plugin.version>
    <maven.cucumber-reporting.version>5.7.4</maven.cucumber-reporting.version>
    <jacoco-maven-plugin.version>0.8.12</jacoco-maven-plugin.version>
    <!-- Plugins End -->
    <jacoco.unit>
			${project.build.directory}/coverage-reports/jacoco-unit.exec</jacoco.unit>
    <jacoco.integration>
			${project.build.directory}/coverage-reports/jacoco-it.exec</jacoco.integration>
    <jacoco.aggregate>
			${project.build.directory}/coverage-reports/aggregate.exec</jacoco.aggregate>
  </properties>
  <distributionManagement>
    <repository>
      <id>releases</id>
      <name>Internal Releases</name>
      <url>${deploy.release.repo.url}</url>
    </repository>
    <snapshotRepository>
      <id>snapshots</id>
      <name>Internal Snapshots</name>
      <url>${deploy.snapshot.repo.url}</url>
    </snapshotRepository>
  </distributionManagement>

</project>

